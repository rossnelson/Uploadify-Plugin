<% title "Photos" %>

<%= stylesheet( 'dust_album','uploadify') %>

<table>
  <tr>
    <th>Title</th>
    <th>Desc</th>
    <th>Date Shot</th>
    <th>Location</th>
  </tr>
  <% for photo in @photos %>
    <tr>
      <td><%=h photo.title %></td>
      <td><%=h photo.desc %></td>
      <td><%=h photo.date_shot %></td>
      <td><%=h photo.location %></td>
      <td><%= link_to "Show", photo %></td>
      <td><%= link_to "Edit", edit_photo_path(photo) %></td>
      <td><%= link_to "Destroy", photo, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Photo", new_photo_path %></p>
