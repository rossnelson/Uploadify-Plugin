<%= stylesheet( 'dust_album','uploadify') %>

<% form_for @photo do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :desc %><br />
    <%= f.text_area :desc %>
  </p>
  <p>
    <%= f.label :date_shot %><br />
    <%= f.date_select :date_shot %>
  </p>
  <p>
    <%= f.label :location %><br />
    <%= f.text_field :location %>
  </p>
  <p><%= f.submit %></p>
<% end %>
