<%= stylesheet( 'dust_album','uploadify') %>

<% form_for @album do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
	<p>
		<%= f.label :filename %><br />
    <%= f.text_field :filename %>
	</p>
  <p>
    <%= f.label :desc %><br />
    <%= f.text_area :desc %>
  </p>
  <p><%= f.submit %></p>
<% end %>
